package mystuff;

public class Cylinder extends Circle{
	protected double height;
	protected double radius; 
	protected Point point;
	
	
	
	public Cylinder(){
		height = 1.0;
		radius = 1.0;
		point = new Point();
				
	}
	public Cylinder(double x, double y, double r, double h){
		height = h;
		radius = r;
		point = new Point(x, y);
				
	}
	
		
